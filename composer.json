            "type": "git"
        },
        "unl_user": {
          "url": "https://github.com/unlcms/unl_user.git",
          "type": "git"
        },
        "unl_breadcrumbs": {
          "url": "https://github.com/unlcms/unl_breadcrumbs.git",
          "type": "git"
        },
        "unl_multisite": {
          "url": "https://github.com/unlcms/unl_multisite.git",
